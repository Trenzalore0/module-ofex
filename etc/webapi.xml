<?xml version="1.0"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/action/:argument" method="GET">
        <service class="Trenzalore\Ofex\Api\ActionInterface" method="get"/>
        <resources>
            <resource ref="self"/>
        </resources>
    </route>
</routes>
